   "source": [
    "[908. Smallest Range I](https://leetcode.com/problems/smallest-range-i/)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "class Solution:\n",
    "    def smallestRangeI(self, A, K: int) -> int:\n",
    "        max_, min_ = max(A), min(A)\n",
    "        \n",
    "        return max(0, (max_ - K) - (min_ + K))"
   ]
  },
