   "source": [
    "[367. Valid Perfect Square](https://leetcode.com/problems/valid-perfect-square/)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "class Solution:\n",
    "    def isPerfectSquare(self, num: int) -> bool:\n",
    "        r = num\n",
    "        while r*r > num:\n",
    "            r = (r + num/r) // 2\n",
    "        return r*r == num"
   ]
