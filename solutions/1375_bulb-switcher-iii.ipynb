   "source": [
    "[1375. Bulb Switcher III](https://leetcode.com/problems/bulb-switcher-iii/)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "class Solution:\n",
    "    def numTimesAllBlue(self, A) -> int:\n",
    "        res = r = 0\n",
    "        for i, a in enumerate(A, 1):\n",
    "            r = max(r, a)\n",
    "            res += r == i\n",
    "        \n",
    "        return res"
   ]
  },
