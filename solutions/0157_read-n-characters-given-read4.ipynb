    "class Solution:\n",
    "    def read(self, s, n):\n",
    "        r = read4(s)\n",
    "        if r >= n:\n",
    "            return n\n",
    "        elif r < 4:\n",
    "            return r\n",
    "        return 4 + read(buf[4:], n - 4)"
