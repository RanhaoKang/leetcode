   "source": [
    "[553. Optimal Division](https://leetcode.com/problems/optimal-division/)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "class Solution:\n",
    "    def optimalDivision(self, nums) -> str:\n",
    "        A = list(map(str, nums))\n",
    "        if len(A) <= 2:\n",
    "            return '/'.join(A)\n",
    "        return A[0] + '/(' + '/'.join(A[1:]) + ')'"
   ]
  },
