   "source": [
    "[810. Chalkboard XOR Game](https://leetcode.com/problems/chalkboard-xor-game/)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "metadata": {},
   "outputs": [],
   "source": [
    "class Solution:\n",
    "    def xorGame(self, nums) -> bool:\n",
    "        xor = 0\n",
    "        for i in nums: xor ^= i\n",
    "        return xor == 0 or len(nums) % 2 == 0"
   ]
  },
  {
