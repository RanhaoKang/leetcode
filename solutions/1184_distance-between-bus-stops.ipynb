   "source": [
    "[1184. Distance Between Bus Stops](https://leetcode.com/problems/distance-between-bus-stops/)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "class Solution:\n",
    "    def distanceBetweenBusStops(self, dis, s: int, e: int) -> int:\n",
    "        s, e = min(s, e), max(s, e)\n",
    "        return min(sum(dis[s:e]), sum(dis) - sum(dis[s:e]))"
   ]
