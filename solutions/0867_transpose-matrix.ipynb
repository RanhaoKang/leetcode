   "source": [
    "[867. Transpose Matrix](https://leetcode.com/problems/transpose-matrix/)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "metadata": {},
   "outputs": [],
   "source": [
    "class Solution:\n",
    "    def transpose(self, A):\n",
    "        h, w = len(A), len(A[0])\n",
    "        return [[A[i][j] for i in range(h)] for j in range(w)]"
   ]
  },
